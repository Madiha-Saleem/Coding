#include<stdio.h>
int main(){
    int n,i,a,f=0,key;
    printf("Enter the no of elements in array : ");
    scanf("%d",&n);
    int arr[n];
    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    printf("Enter the element to search: ");
    scanf("%d", &key);
    for(i=0;i<n;i++){
        if(arr[i]==key){
            a=i; 
            f=1;
            break;
        }
    }
    if(f==1)
    printf("Element found at %d",a);
    else
    printf("Element not found");
}
