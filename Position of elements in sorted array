class Solution {
    public int[] searchRange(int[] nums, int target) {
        int i,start=-1,end=-1;
        for(i=0;i<nums.length;i++)
        {
            if(target==nums[i]){
                if (start == -1)
            start=i;
            end=i;
            }
        }
        return new int[]{start, end};
    }
}
